#!/bin/sh

# wrap.test is part of the GNU m4 testsuite

. ${srcdir}/defs

cat ${srcdir}/wrap.m4 >in

cat <<\EOF >ok

No. 33: The End.
Wrapper no. 2
Wrapper no. 1
Wrapper no. 3
Wrapper no. 4
EOF

M4PATH=$srcdir:$srcdir/../tests $M4 -d in >out 2>err
sed -e "s, ../../src/m4:, m4:," err >sederr && mv sederr err
$CMP -s out ok
