#!/bin/sh

# iso8859.test is part of the GNU m4 testsuite

. ${srcdir}/defs

cat <<\EOF >in
include(iso8859.m4)dnl
EOF

cat <<\EOF >ok
# Texting quotes
DEFINE			
CHANGEQUOTE(«,»)	
0 TEST			# TEST
1 test			# test
2 «test»		# «test»
3 ««test»»		# ««test»»
CHANGEQUOTE(«««,»»»)	
0 TEST			# TEST
1 «TEST»		# «TEST»
2 ««TEST»»		# ««TEST»»
3 test			# test
# Test use of all iso8859 characters except NUL ` '
Length of string is: 253
Comparing strings: MATCH
# NUL does not pass through
This will be seen.
EOF

M4PATH=$srcdir:$srcdir/../examples $M4 -d in >out

$CMP -s out ok
