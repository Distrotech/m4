#! /bin/sh
# @configure_generated@
# Wrapper around a non installed m4 to make it work as an installed one.

"@top_buildpath@/src/m4" \
      --module-directory="@top_buildpath@/modules" \
      ${1+"$@"} 2>/tmp/m4-$$
status=$?
# Normalize stderr.
sed 's,^[lt-]*m4[.ex]*:,m4:,' /tmp/m4-$$ >&2
rm /tmp/m4-$$

exit $status
